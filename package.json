{
  "name": "hisey-admin-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:prod": "vue-cli-service build",
    "build:test": "vue-cli-service build --mode test",
    "build:dev": "vue-cli-service build --mode dev",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "dayjs": "^1.8.17",
    "vue": "^2.6.10",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.12.2",
    "echarts": "^4.5.0",
    "element-ui": "^2.12.0",
    "eslint": "^5.16.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "js-cookie": "^2.2.1",
    "node-sass": "^4.13.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "sass-loader": "^8.0.0",
    "store": "^2.0.12",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.10",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "plugins": [
      "html"
    ],
    "extends": "plugin:vue/base",
    "parserOptions": {
      "sourceType": "module",
      "parser": "babel-eslint"
    },
    "rules": {
      "comma-spacing": 0,
      "keyword-spacing": 0,
      "no-cond-assign": 2,
      "no-constant-condition": 2,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-empty": 2,
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        0,
        "unix"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        2,
        {
          "args": "none"
        }
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
